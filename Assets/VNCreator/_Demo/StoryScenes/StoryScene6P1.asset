  - guid: 3dd57d3e-0de7-494f-9133-64d76164acbc
    characterSpr: {fileID: 0}
    characterName: KUT
    dialogueText: "Did you say hi to everybody? Aren\u2019t they great?? They\u2019re
      like family! Or what I think families are supposed to be like. "
    backgroundSpr: {fileID: 0}
    startNode: 0
    endNode: 0
    choices: 1
    choiceOptions:
