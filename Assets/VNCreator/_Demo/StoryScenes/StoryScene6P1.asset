  - guid: 132082ec-a0c7-49e2-a061-b684fa466c69
    characterSpr: {fileID: 0}
    characterName: LARU
    dialogueText: "<i>H-he just\u2026.said that his dog\u2026and that\u2019s where\u2013I
      am not touching that. I take it back, not an alien, that is too normal! </i>"
    backgroundSpr: {fileID: 0}
    startNode: 0
    endNode: 0
    choices: 1
    choiceOptions:
    - 
    nodePosition:
      serializedVersion: 2
