  - guid: 24db8360-2b6d-40b8-8241-4a3cabece08c
    characterSpr: {fileID: 0}
    characterName: KUT
    dialogueText: "I\u2019ll let you get acquainted with everybody! Feel free to
      shake fronds, or pet anybody. I\u2019ll go make us tea."
    backgroundSpr: {fileID: 0}
    startNode: 0
    endNode: 0
    choices: 1
